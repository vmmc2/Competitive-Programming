#include <bits/stdc++.h>

using namespace std;

int main(){
  int numcasos;
  int x, y, i;
  scanf("%d", &numcasos);
  for(i = 1; i <= numcasos; i++){
    scanf("%d %d", &x, &y);
    if(x > y){
      printf(">\n");
    }
    else if(x == y){
      printf("=\n");
    }
    else if(x < y){
      printf("<\n");
    }
  }
  return 0;
}
