#include <bits/stdc++.h>
#include <cmath>
#define pb push_back
#define pii pair<int,int>
#define INF 999999

using namespace std;

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    
    double amount;
    long long coins[] = {5,10,20,50,100,200,1000,2000,5000,10000};
    while(cin >> amount){
        if(amount == 0.00) break; //fim dos casos de teste e devemos encerrar o programa
        int new_amount = (int)(amount * 100);
        long long dp[11][new_amount + 1];
        //pre-process....
        for(int i = 0; i <= 10; i++){
            dp[i][0] = 1;
        }
        for(int i = 1; i <= new_amount; i++){
            dp[0][i] = 0;
        }
        for(int i = 1; i <= 10; i++){
            for(int j = 1; j <= new_amount; j++){
                if(coins[i - 1] > j){
                    dp[i][j] = dp[i - 1][j];
                }else{
                    dp[i][j] = dp[i - 1][j] + dp[i][j - coins[i - 1]];
                }
            }
        }
        printf("%.2lf                  %lld\n", amount, dp[10][new_amount]);
    }
    return 0;
}
